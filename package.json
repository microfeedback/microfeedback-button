{
  "name": "wishes-button",
  "version": "0.0.0",
  "description": "A simple feedback button/widget. For use with a Wishes microservice.",
  "scripts": {
    "dev": "NODE_ENV=development rollup -c --watch",
    "build": "npm-run-all --parallel build:development build:production",
    "build:production": "NODE_ENV=production rollup -c",
    "build:development": "NODE_ENV=build rollup -c",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "pretest": "npm run build:development",
    "lint": "eslint src",
    "test": "npm run lint && NODE_ENV=test ava test --serial",
    "test:debug": "NODE_ENV=test iron-node ./node_modules/ava/profile.js test/tests.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "browser": "dist/wishes-button.js",
  "main": "dist/wishes-button.js",
  "repository": "wishesjs/wishes-button",
  "authors": [
    "Steven Loria (https://github.com/sloria)",
    "Lauren Barker (https://github.com/laurenbarker)"
  ],
  "license": "MIT",
  "keywords": [
    "user",
    "customer",
    "feedback",
    "ui",
    "widget",
    "button",
    "wishes"
  ],
  "devDependencies": {
    "autoprefixer": "^7.0.0",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "browser-env": "^2.0.31",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-filesize": "^1.3.2",
    "rollup-plugin-postcss": "^0.4.1",
    "rollup-plugin-serve": "^0.3.0",
    "rollup-plugin-uglify": "^1.0.2",
    "rollup-watch": "^3.2.2",
    "sinon": "^2.2.0",
    "syn": "^0.5.0"
  },
  "ava": {
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  }
}
