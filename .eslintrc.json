{
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "plugins": [
        "import"
    ],
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "import/extensions": 0,
        "no-unused-expressions": 0,
        "arrow-parens": 0,
        "no-plusplus": 0
    }
}
